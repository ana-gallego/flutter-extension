{
  "name": "andromeda",
  "displayName": "Andromeda",
  "description": "Extensi√≥n para desarrollo en flutter",
  "version": "0.0.8",
  "publisher": "ana-gallego",
  "repository": {
    "type": "git",
    "url": "https://github.com/ana-gallego/flutter-extension.git"
  },
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.75.0"
  },
  "keywords": [
    "Flutter",
    "Dart"
  ],
  "categories": [
    "Other",
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.wrapInContainer",
        "title": "Wrap in Container"
      },
      {
        "command": "extension.wrapInStack",
        "title": "Wrap in Stack"
      },
      {
        "command": "extension.wrapInColumn",
        "title": "Wrap in Column"
      },
      {
        "command": "extension.wrapInRow",
        "title": "Wrap in Row"
      },
      {
        "command": "extension.wrapInListView",
        "title": "Wrap in ListView"
      },
      {
        "command": "extension.wrapInPositioned",
        "title": "Wrap in Positioned"
      },
      {
        "command": "extension.wrapInExpanded",
        "title": "Wrap in Expanded"
      },
      {
        "command": "extension.wrapInText",
        "title": "Wrap in Text"
      }
    ],
    "menus": {
      "explorer/context": []
    },
    "keybindings": [
      {
        "command": "extension.wrapInContainer",
        "key": "alt+c",
        "mac": "alt+c",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.wrapInStack",
        "key": "alt+s",
        "mac": "alt+s",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.wrapInColumn",
        "key": "alt+x",
        "mac": "alt+x",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.wrapInRow",
        "key": "alt+r",
        "mac": "alt+r",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.wrapInListView",
        "key": "alt+l",
        "mac": "alt+l",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.wrapInPositioned",
        "key": "alt+p",
        "mac": "alt+p",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.wrapInExpanded",
        "key": "alt+e",
        "mac": "alt+e",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.wrapInText",
        "key": "alt+t",
        "mac": "alt+t",
        "when": "editorTextFocus"
      }
    ],
    "snippets": [
      {
        "language": "dart",
        "path": "./src/snippets/snippets.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/q": "^1.5.4",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "vscode": "^1.1.37"
  },
  "dependencies": {
    "express-es6-template-engine": "^2.2.3",
    "path": "^0.12.7",
    "q": "^1.5.1"
  }
}